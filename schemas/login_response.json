{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "msg": { "type": "string" },
    "token": { "type": "string" },
    "user": {
      "type": "object",
      "properties": {
        "idusuarios": { "type": "integer" },
        "email": { "type": "string", "format": "email" }
      },
      "required": ["idusuarios", "email"],
      "additionalProperties": false
    }
  },
  "required": ["msg", "token", "user"],
  "additionalProperties": false
}
